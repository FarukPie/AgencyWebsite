@model List<SocialMedia>
<div class="pixel-main-menu" id="sticker">
	<div class="classy-nav-container breakpoint-off">
		<div class="container-fluid">
			<!-- Menu -->
			<nav class="classy-navbar justify-content-between" id="pixelNav">

				<!-- Nav brand -->
				<a href="/Default/Index" class="nav-brand">
					<img src="/pixel-master/img/core-img/spoilerLogo.png" alt="" style="width: 140px; height: 140px; object-fit: contain;">
				</a>

				<!-- Navbar Toggler -->
				<div class="classy-navbar-toggler">
					<span class="navbarToggler"><span></span><span></span><span></span></span>
				</div>

				<!-- Menu -->
				<div class="classy-menu">

					<!-- Close Button -->
					<div class="classycloseIcon">
						<div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
					</div>

					<!-- Nav Start -->
					<div class="classynav">
						<!-- Top Social Info -->
						@foreach (var item in Model)
						{
							<div class="top-social-info ml-5">
								<a href="@item.item"><i class="fa fa-@item.imgUrl"></i></a>

							</div>

						}

					</div>
					<!-- Nav End -->
				</div>
			</nav>
		</div>
	</div>
</div>