@model List<TwoPhoto>
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Layout/Index.cshtml";
}

<div class="row">

	<div class="col-lg-12 grid-margin stretch-card">
		<div class="card">
			<div class="card-body">
				<h3 class="card-title">Fotoğraflar Bölümü</h3>
				<div class="table-responsive">
					<table class="table">
						<thead>
							<tr>
								<th>Başlık</th>
								<th>Resim Url</th>


							</tr>
						</thead>
						<tbody>

							@foreach (var item in Model)
							{
								<tr>
									<td>@item.Title</td>
									<td>@item.imgUrl</td>

									<td><a href="/Photo/UpdatePhoto/@item.TwoPhotoId" class="btn btn-outline-success">Update</a></td>
								</tr>

							}

						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>